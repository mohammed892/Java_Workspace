package homework;

public class Summation {

	public static void main(String[] args) {
		// Write a Java program that take a number n from the user and print the summation of 1 to n.
		
	
	        int n = 100;
	        int sum = 0;
	 
	        for (int i = 0; i <= n; i++) {
	              sum = sum + i;
	        }
	        System.out.println(sum);
	

	}

}
